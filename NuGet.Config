<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <clear />
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
        <add key="VelloNativeLocal" value="artifacts/nuget" />
    </packageSources>
    <packageSourceMapping>
        <packageSource key="nuget.org">
            <package pattern="*" />
        </packageSource>
        <packageSource key="VelloNativeLocal">
            <package pattern="VelloSharp" />
            <package pattern="VelloSharp.Native.*" />
            <package pattern="VelloSharp.*" />
        </packageSource>
    </packageSourceMapping>
</configuration>
