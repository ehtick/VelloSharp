{
  "metadata": [
    {
      "src": [
        {
          "src": "../..",
          "files": [
            "bindings/**/*.csproj",
            "src/**/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/extern/**",
            "**/tests/**",
            "**/samples/**",
            "bindings/VelloSharp.Avalonia.Browser/**",
            "bindings/VelloSharp.Maui/**",
            "src/VelloSharp.Maui.Core/**"
          ]
        }
      ],
      "dest": "obj/api",
      "outputFormat": "markdown",
      "disableGitFeatures": true,
      "properties": {
        "Configuration": "Release"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/index.md"
        ],
        "dest": "dotnet-api"
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "globalMetadata": {
      "_appTitle": "VelloSharp API Reference"
    },
    "dest": "obj/site"
  }
}
