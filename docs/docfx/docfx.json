{
  "metadata": [
    {
      "src": [
        {
          "src": "../..",
          "files": [
            "bindings/**/*.csproj",
            "src/**/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/extern/**",
            "**/tests/**",
            "**/samples/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": true,
      "properties": {
        "Configuration": "Release",
        "EnableWindowsTargeting": "true"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml"
        ]
      },
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "globalMetadata": {
      "_appTitle": "VelloSharp Documentation",
      "_enableSearch": true
    },
    "dest": "_site"
  }
}
